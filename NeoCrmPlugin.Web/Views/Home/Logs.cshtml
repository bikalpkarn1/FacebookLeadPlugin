@model  List<NeoCrmPlugin.Data.Models.Log>

@{
    Layout = "~/Views/Shared/_Layout_old.cshtml";
    //  ViewData["Title"] = "Privacy Policy";
}
<div class="row">
    <div class="col-md-12"> 
        <div class="cards">
            <div class="card-header text-white bg-primary ">Logs</div>
            <div class="card-body">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <td> Level </td>
                            @*<td> Log Event </td>*@
                            <td> Log Time </td>
                            <td> Message </td>

                            <td> Properties</td>
                        </tr>
                    </thead>
                    <tbody style="font-size:13px;">
                        @foreach (var bod in Model)
                        {
                            <tr>
                                <td style="width:5%">@bod.Level</td>
                                <td style="width:11%">@bod.TimeStamp.ToString("dd-MMM-yyyy  h:mm tt")</td>
                                <td style="width:25%">@bod.Message</td>
                                <td><a href="#" target="_blank" style="width:60%">@bod.Properties</a></td>
                            </tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>


@section Scripts {
    <script type="text/javascript">
    $(document).ready(function () {

        $('.table').DataTable();
    });
    </script>
    }